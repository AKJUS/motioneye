#: templates/main.html:100
msgid "agordojn"
msgstr "nastavenia"

#: templates/main.html:101
msgid "ŝanĝi uzanton"
msgstr "prepnúť používateľa"

#: templates/main.html:103
msgid "forigi kameraon"
msgstr "odstrániť kameru"

#: templates/main.html:104
msgid "Apliki"
msgstr "Použiť"

#: templates/main.html:123
msgid "preferoj de uzanto"
msgstr "predvoľby používateľa"

#: templates/main.html:124
msgid "Preferoj"
msgstr "Predvoľby"

#: templates/main.html:129
msgid "Aranĝo Kolumnoj"
msgstr "Rozloženie stĺpcov"

#: templates/main.html:131
msgid "agordas la nombron da kolumnoj uzataj por aranĝi la fotilojn"
msgstr "určuje počet stĺpcov, v ktorých sú zobrazované výstupy z kamier"

#: templates/main.html:134
msgid "Fiksi Kadrojn Vertikale"
msgstr "Zmenšiť náhľady zvisle"

#: templates/main.html:136
msgid "kontrolas ĉu framfojoj povas esti reduktitaj por vertikale konveni al la fenestro aŭ ne"
msgstr "ovláda, či sa majú veľkosti náhľadov zmenšiť tak, aby sa zmenšili do okna alebo nie"

#: templates/main.html:139
msgid "Aranĝo linioj"
msgstr "Rozloženie riadkov"

#: templates/main.html:141
msgid "agordas la nombron da linioj uzataj por aranĝi la fotilojn"
msgstr "určuje počet riadkov, v ktorých sú zobrazované výstupy z kamier"

#: templates/main.html:144
msgid "kadro-variatoro"
msgstr "Tlmič snímkovej frekvencie"

#: templates/main.html:146
msgid "reduktas framfrekvencon por ŝpari retan larĝan bandon kaj trafikon (ne funkcias kun simplaj MJPEG-fotiloj)"
msgstr "zníži globálnu snímkovú frekvenciu, aby sa tak nezaťažovala sieť (nefunguje s jednoduchými MJPEG kamerami)"

#: templates/main.html:149
msgid "Rezolucio-variatoro"
msgstr "Tlmič rozlíšenia"

#: templates/main.html:151
msgid "reduktas la efektivan rezolucion de ĉiuj fotiloj por ŝpari retan larĝan bandon kaj trafikon (ne funkcias en simplaj MJPEG-fotiloj)"
msgstr "zníži skutočné rozlíšenie všetkých kamier, čím sa predchádza zaťaženiu siete (nefunguje s jednoduchými MJPEG kamerami)"

#: templates/main.html:161
msgid "ĝeneralaj parametroj, komunaj al ĉiuj fotiloj"
msgstr "všeobecné nastavenia, spoločné pre všetky kamery"

#: templates/main.html:162
msgid "Ĝeneralaj Agordoj"
msgstr "Všeobecné nastavenia"

#: templates/main.html:167
msgid "Administranto"
msgstr "Účet administrátora"

#: templates/main.html:169
msgid "la uzantnomo uzata por agordi la sistemon"
msgstr "meno používateľa používané pri konfigurovaní systému"

#: templates/main.html:172 templates/main.html:182 templates/main.html:508
msgid "Pasvorto"
msgstr "Heslo"

#: templates/main.html:174
msgid "pasvorto de administranto"
msgstr "heslo administrátora"

#: templates/main.html:177
msgid "Observanto"
msgstr "Účet dohľadu"

#: templates/main.html:179
msgid "la uzantnomo por uzi por la monitorado"
msgstr "meno používateľa pre videodohľad"

#: templates/main.html:184
msgid "la pasvorto de la uzanto de monitorado (lasu blankan por monitorado sen pasvorto)"
msgstr "heslo používateľa vykonávajúceho dohľad (ponechajte prázdne pre účet bez hesla)"

#: templates/main.html:193
msgid "motionEye Versio"
msgstr "Verzia motionEye"

#: templates/main.html:197
msgid "Motion Versio"
msgstr "Verzia Motion"

#: templates/main.html:201
msgid "Operaciumo Versio"
msgstr "Verzia OS"

#: templates/main.html:206
msgid "Programara Ĝisdatigo"
msgstr "Softvérová aktualizácia"

#: templates/main.html:208
msgid "kontrolas novajn versiojn kaj plenumas ĝisdatigojn"
msgstr "skontroluje dostupnosť novšej verzie a vykoná aktualizáciu"

#: templates/main.html:212
msgid "Nutro"
msgstr "Napájanie"

#: templates/main.html:213
msgid "Fermu"
msgstr "Vypnúť"

#: templates/main.html:214
msgid "malŝaltas la sistemon"
msgstr "vypne systém"

#: templates/main.html:218
msgid "Rekomencu"
msgstr "Reštartovať"

#: templates/main.html:219
msgid "rekomencas la sistemon"
msgstr "reštartuje systém"

#: templates/main.html:225
msgid "Agordo"
msgstr "Nastavenia"

#: templates/main.html:226
msgid "Sekurkopio"
msgstr "Zálohovať"

#: templates/main.html:227
msgid "kreas dosieron kun la nuna agordo por ke vi konservu ĝin surloke"
msgstr "vytvorí súbor s aktuálnym nastavením, ktorý si môžete uložiť"

#: templates/main.html:231
msgid "Restaŭri"
msgstr "Obnoviť"

#: templates/main.html:232
msgid "restarigas la agordon el antaŭe konservita rezerva dosiero"
msgstr "obnoví konfiguráciu zo súboru so zálohou vytvorenou v minulosti"

#: templates/main.html:260
msgid "aktivigu ĉi tion, se vi volas uzi ĉi tiun kameraon"
msgstr "zapnite túto voľbu, ak chcete použiť toto kamerové zariadenie"

#: templates/main.html:261
msgid "Video-Aparato"
msgstr "Zariadenie videa"

#: templates/main.html:266 templates/main.html:596 templates/main.html:613
msgid "Kamerao Nomo"
msgstr "Názov kamery"

#: templates/main.html:267
msgid "kameraa nomo..."
msgstr "názov kamery ..."

#: templates/main.html:268
msgid "alias pri ĉi tiu kamera aparato"
msgstr "prezývka tohto kamerového zariadenia"

#: templates/main.html:271
msgid "Kamerao ID"
msgstr "ID kamery"

#: templates/main.html:273
msgid "la kamerao identiga numero"
msgstr "identifikačné číslo kamery"

#: templates/main.html:276
msgid "Kamerao aparato"
msgstr "Kamerové zariadenie"

#: templates/main.html:278
msgid "Eniga aparato (ekz. : /dev/video) or URL (ekz. : rtsp://192.168.1.1/stream)"
msgstr "Vstupné zariadenie (napr. /dev/video) alebo URL adresa (napr. rtsp://192.168.1.1/stream)"

#: templates/main.html:281
msgid "Kamerao Tipo"
msgstr "Typ kamery"

#: templates/main.html:288
msgid "Aŭtomata Brilo"
msgstr "Automatický jas"

#: templates/main.html:290
msgid "ebligas aŭtomatan brilon de programaro (rekomendita nur por kameraoj sen aŭtomata brilo)"
msgstr "povolí automatický softvérový jas (odporúčané iba pre kamery bez automatického jasu)"

#: templates/main.html:296
msgid "Video-rezolucio"
msgstr "Rozlíšenie videa"

#: templates/main.html:301
msgid "la video-rezolucio (pli grandaj valoroj produktas pli bonan kvaliton, sed postulas pli da CPU-potenco, pli granda stokado kaj larĝa de bando)"
msgstr "rozlíšenie videa (vyššie hodnoty znamenajú vyššiu kvalitu, ale vyžadujú viac procesorového času, viac priestoru na úložisku a rýchlejšiu sieť)"

#: templates/main.html:304
msgid "Larĝeco"
msgstr "Šírka"

#: templates/main.html:306
msgid "enigu kutiman rezolucian larĝon"
msgstr "zadajte vlastnú hodnotu pre rozlíšenie šírky"

#: templates/main.html:309
msgid "Alteco"
msgstr "Výška"

#: templates/main.html:311
msgid "enigu kutiman rezolucian altecon"
msgstr "zadajte vlastnú hodnotu pre rozlíšenie výšky"

#: templates/main.html:314
msgid "Video-Rotacio"
msgstr "Otočenie videa"

#: templates/main.html:323
msgid "uzu ĉi tion por turni la kaptitan bildon, se via kamerao ne estas poziciigita ĝuste"
msgstr "použite túto voľbu na otočenie zaznamenaného obrazu, ak vaša kamera nemá správnu orientáciu"

#: templates/main.html:326
msgid "Kadrofrekvenco"
msgstr "Snímková frekvencia"

#: templates/main.html:328
msgid "agordas la nombron de kadroj kaptitaj de la kamerao ĉiun sekundon (pli altaj valoroj produktas pli mildajn filmetojn sed postulas pli da CPU-potenco, pli grandan stokadon kaj larĝan bandon)"
msgstr "nastavuje počet snímkov za sekundu zaznamenaných kamerou (vyššie hodnoty znamenajú plynulejšie videá, ale vyžadujú viac procesorového času, väčšie úložisko a vyššiu priepustnosť siete)"

#: templates/main.html:334
msgid "Privateca masko"
msgstr "Maska súkromia"

#: templates/main.html:336
msgid "ebligas maskeradon de bildoj por eviti registradon de iuj bildaj areoj por protekti privatecon"
msgstr "zapne maskovanie obrázka, ktorý vám umožní nastaviť časti obrázka, ktoré sa nebudú nahrávať za účelom ochranu súkromia"

#: templates/main.html:341 templates/main.html:989
msgid "Redakti maskon"
msgstr "Upraviť masku"

#: templates/main.html:342 templates/main.html:990
msgid "Konservi maskon"
msgstr "Uložiť masku"

#: templates/main.html:345
msgid "alklaku ĉi tiun butonon por ebligi/malebligi la maskan redaktilon"
msgstr "kliknite na toto tlačidlo na zapnutie/vypnutie editora masky"

#: templates/main.html:356
msgid "Ekstraj Elektoj"
msgstr "Ďalšie voľby"

#: templates/main.html:358
msgid "vi povas aldoni ĉi tie iujn ajn kromajn eblojn por la &quot;motion&quot; demono (uzu la formaton &quot;nomo valoro&quot;, unu opcion po linio)"
msgstr "tu môžete pridať daľsie voľby pre \"motion\" (použite formát \"názov hodnota\", jednu voľbu na riadok)"

#: templates/main.html:367
msgid "elektu kie kaj kiel viaj amaskomunikiloj estas konservitaj"
msgstr "vyberte kam a ako budú multimédiá ukladané"

#: templates/main.html:368
msgid "Stokado de dosieroj"
msgstr "Úložisko súborov"

#: templates/main.html:373
msgid "Stokada Aparato"
msgstr "Zariadenie úložiska"

#: templates/main.html:378
msgid "indikas la stokan aparaton, kie la bildo kaj video-dosieroj estos konservitaj"
msgstr "vyberá, na ktoré zariadenie úložiska budú obrázky a video súbory ukladané"

#: templates/main.html:381
msgid "Retservilo"
msgstr "Sieťový server"

#: templates/main.html:383
msgid "la adreso de la retservilo (IP-adreso aŭ nomo)"
msgstr "adresa sieťového servera (IP adresa alebo meno hostiteľa)"

#: templates/main.html:386
msgid "SMB Protokolo-Versio"
msgstr "Verzia protokolu SMB"

#: templates/main.html:396
msgid "elektu la SMB-version, kiun vi povas uzi (testu la malsamajn eblojn kaj vidu, kiu funkcias plej bone kun via NAS)"
msgstr "vyberá verziu protokolu SMB, ktorý sa má použiť (vyskúšajte rôzne možnosti a zistite, ktorá funguje najlepšie s vašim systémom NAS)"

#: templates/main.html:399
msgid "Kunhava nomo"
msgstr "Názov zdieľania"

#: templates/main.html:401
msgid "la nomo de la interŝanĝa reto"
msgstr "názov sieťového zdieľania"

#: templates/main.html:404 templates/main.html:503
msgid "Uzantnomo"
msgstr "Meno používateľa pre zdieľanie"

#: templates/main.html:406
msgid "la uzantnomo kiu estu provizita kiam vi aliras la reton dosierujon (lasu malplena se neniu uzantnomo estas bezonata)"
msgstr "meno používateľa, ktoré je potrebné zadať pre prístup ku zdieľanému prostriedku (nechajte prázdne, ak sa zadávanie mena nevyžaduje)"

#: templates/main.html:409
msgid "Pasvorton"
msgstr "Heslo pre zdieľanie"

#: templates/main.html:411
msgid "la pasvorto bezonata de la reto dosierujo (lasi malplena se neniu pasvorto estas bezonata)"
msgstr "heslo vyžadované sieťovým zdieľaním (nechajte prázdna, ak zadanie hesla nie je potrebné)"

#: templates/main.html:414
msgid "Radika dosierujo"
msgstr "Koreňový adresár"

#: templates/main.html:416
msgid "la radika vojo (sur la elektita stokada aparato), kie la dosieroj estos konservitaj"
msgstr "cesta ku koreňu (na vybranom zariadení úložiska), kam budú súbory ukladané"

#: templates/main.html:420
msgid "Testi la reton dosierujon"
msgstr "Otestovať zdieľanie"

#: templates/main.html:421
msgid "alklaku ĉi tiun butonon por testi la retan kunlokon post kiam vi plenigis la postulatajn detalojn"
msgstr "kliknite na toto tlačidlo na otestovanie pripojenia ku zdieľanému prostriedku po tom, ako ste vyplnili všetky potrebné polia"

#: templates/main.html:427
msgid "Uzado de Disko"
msgstr "Využitie disku"

#: templates/main.html:431
msgid "la uzata/tuta grandeco de la disko, kie loĝas la radika dosierujo"
msgstr "použitá/celková veľkosť disku, na ktorom sa nachádza koreňový adresár"

#: templates/main.html:437
msgid "Alŝutu Mediajn Dosierojn"
msgstr "Nahrávať multimediálne súbory"

#: templates/main.html:439
msgid "aktivigu ĉi tiun opcion, se vi volas, ke viaj amaskomunikiloj estu alŝutitaj al ekstera servo"
msgstr "zapnite túto voľbu, ak chcete aby sa multimediálne súbory nahrávali na externú službu"

#: templates/main.html:442
msgid "Alŝutu Bildojn"
msgstr "Nahrávať obrázky"

#: templates/main.html:444
msgid "aktivigu ĉi tiun opcion, se vi volas, ke la bildoj elŝutiĝu al ekstera servo"
msgstr "zapnite túto voľbu, ak chcete aby sa obrázky nahrávali na externú službu"

#: templates/main.html:447
msgid "Alŝutu Filmojn"
msgstr "Nahrávať videá"

#: templates/main.html:449
msgid "ebligu ĉi tion, se vi volas, ke filmetoj estu alŝutitaj al ekstera servo"
msgstr "zapnite túto voľbu, ak chcete aby sa videá nahrávali na externú službu"

#: templates/main.html:452
msgid "Alŝuta Servo"
msgstr "Nahrávacia služba"

#: templates/main.html:455
msgid "Servilo FTP"
msgstr "Server FTP"

#: templates/main.html:456
msgid "Servilo SFTP"
msgstr "Server SFTP"

#: templates/main.html:457
msgid "Servilo HTTP"
msgstr "Server HTTP"

#: templates/main.html:458
msgid "Servilo HTTPS"
msgstr "Server HTTPS"

#: templates/main.html:465
msgid "elektu servon, al kiu la mediadosieroj estu alŝutitaj"
msgstr "vyberte službu, na ktorú sa majú nahrávať multimediálne súbory"

#: templates/main.html:468
msgid "Servila Adreso"
msgstr "Adresa servera"

#: templates/main.html:470
msgid "la domajna nomo aŭ IP-adreso de la servilo (ekz. ftp.example.com aŭ 192.168.1.3)"
msgstr "názov domény alebo IP adresa servera (napr. ftp.priklad.com alebo 192.168.1.3)"

#: templates/main.html:473
msgid "Servila haveno"
msgstr "Port"

#: templates/main.html:475
msgid "la haveno por uzi konekti al la servo (lasu ĉi tiun kampon malplena por uzi la defaŭltan valoron)"
msgstr "port, ktorý sa použije pre pripojenie k službe (nechajte prázdne, ak chcete použiť predvolenú hodnotu)"

#: templates/main.html:478
msgid "Metodo"
msgstr "Metóda"

#: templates/main.html:485
msgid "la HTTP-metodo uzi por alŝuti dosierojn"
msgstr "HTTP metóda, ktorá sa použije pri nahrávaní súborov"

#: templates/main.html:488
msgid "Loko"
msgstr "Umiestnenie"

#: templates/main.html:490
msgid "la loko (radika vojo) kie amaskomunikiloj devas esti alŝutitaj (ekz. /files/cam1/)"
msgstr "umiestnenie (cesta ku koreňu), kam majú byť multimediálne súbory nahraté (napr. /subory/cam1/)"

#: templates/main.html:493
msgid "Inkluzivi Subdosierujojn"
msgstr "Vrátane podpriečinkov"

#: templates/main.html:495
msgid "Aktivigu tion por elŝuti plurmediajn dosierojn kun siaj subdosierujoj, anstataŭ meti ilin rekte en la radikan lokon"
msgstr "zapnite túto voľbu pre nahranie multimediálnych súborov spolu s podpriečinkami miesto ich umiestnenia priamo do koreňového umiestnenia."

#: templates/main.html:498
msgid "Purigi la nubon"
msgstr "Vyčistiť cloud"

#: templates/main.html:500
msgid "ebligu ĉi tion forigi amaskomunikilaĵojn dosierojn alŝutitajn al la nubo ankaŭ kiam loka amaskomunikilaro estas forigita konforme al la agordo de persistemo de dosieroj. Nuntempe ĉi tiu opcio nur haveblas por gdrivo."
msgstr "zapnite túto voľbu na odstránenie multimediálnych súborov v cloude vždy, keď sa na základe nastavení životnosti odstraňujú miestne súbory. Aktuálne je táto možnosť dostupná len pre gdrive."

#: templates/main.html:505
msgid "la uzantnomo por la alŝuta servo-konto"
msgstr "meno používateľa účtu nahrávacej služby"

#: templates/main.html:510
msgid "la pasvorto por la alŝuta servo-konto"
msgstr "heslo k účtu nahrávacej služby"

#: templates/main.html:513
msgid "Rajtiga ŝlosilo"
msgstr "Kľúč pre autorizáciu"

#: templates/main.html:515
msgid "la ŝlosilo uzata por aŭtentiĝi kun la alŝuta servo (kutime bezonata nur dum agordo)"
msgstr "kľúč použitý na overenie voči nahrávacej službe (štandardne vyžadované len pri úvodnom nastavovaní)"

#: templates/main.html:521
msgid "Akiri ŝlosilon"
msgstr "Získať kľúč"

#: templates/main.html:524
msgid "alklaku ĉi tie por akiri la ŝlosilan rajtigan servon"
msgstr "kliknite sem a získajte tak autorizačný kľúč služby"

#: templates/main.html:527
msgid "Sekreta Alira Ŝlosilo"
msgstr "Tajný prístupový kľúč"

#: templates/main.html:529
msgid "la AWS-sekreta alira ŝlosilo uzata por aŭtentiĝi kun la alŝuta servo (kutime bezonata nur dum agordo)"
msgstr "tajný prístupový kľúč AWS na overenie voči nahrávacej službe (štandardne vyžadované len pri úvodnom nastavovaní)"

#: templates/main.html:532
msgid "Sitelo"
msgstr "Bucket"

#: templates/main.html:534
msgid "la nomo S3-sitelo por la alŝuta servo (kutime bezonata nur dum agordo)"
msgstr "meno S3 bucketu nahrávacej služby (štandardne vyžadované len pri úvodnom nastavovaní)"

#: templates/main.html:538
msgid "Testi la servon"
msgstr "Otestovať službu"

#: templates/main.html:539
msgid "alklaku ĉi tiun butonon por provi la alŝutan servon post kiam vi plenigis la postulatajn detalojn"
msgstr "kliknite na toto tlačidlo na otestovanie nahrávacej služby po tom, ako vyplníte požadované polia"

#: templates/main.html:545
msgid "alvoki URL"
msgstr "Zavolať webový hook"

#: templates/main.html:547
msgid "ebligu ĉi tion, se vi volas, ke URL estu petata post kreado de amaskomunikila dosiero"
msgstr "zapnite túto voľbu, ak chcete po vytvorení multimediálneho súboru vytvoriť požiadavku na URL adresu"

#: templates/main.html:550 templates/main.html:1111
msgid "Reteja URL"
msgstr "URL adresa webového hooku"

#: templates/main.html:551
msgid "ekz. http://example.com/notify/"
msgstr "napr. http://priklad.com/notify/"

